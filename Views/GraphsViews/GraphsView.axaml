<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="clr-namespace:DevelopmentProject.ViewModels.Graphs"
             xmlns:graphsViews="clr-namespace:DevelopmentProject.Views.Graphs_Views"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="DevelopmentProject.Views.GraphsViews.GraphsView"
             x:DataType="vm:GraphsViewModel">
    <Design.DataContext>
        <vm:GraphsViewModel/>
    </Design.DataContext>
    
    <Grid>
        <graphsViews:RiverSpeedGraphView DataContext="{Binding riverSpeedGraphViewModel}"></graphsViews:RiverSpeedGraphView>
    </Grid>

</UserControl>
